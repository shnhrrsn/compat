        "children": [
          {
            "type": "element",
            "tagName": "p",
            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The parameter passed into the "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/ServiceWorkerGlobalScope/oninstall"
