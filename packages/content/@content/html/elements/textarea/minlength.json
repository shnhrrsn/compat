  "content": {
    "intro": {
      "id": "0d67a5e4ad155b165218de2c1bfad3b1",
      "tree": {
        "type": "root",
        "children": [
          {
            "type": "element",
            "tagName": "p",
            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The "
              },
              {
                "type": "element",
                "tagName": "strong",
                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "minlength"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " attribute defines the minimum number of characters (as UTF-16 code units) the user can enter into an "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/html/elements/input"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "<input>"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " or "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/html/elements/textarea"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "<textarea>"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": ". This must be an integer value 0 or higher. If no minlength is specified, or an invalid value is specified, the input has no minimum length. This value must be less than or equal to the value of "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "href": "/en-US/docs/Web/HTML/Attributes/maxlength"
                },
                "children": [
                  {
                    "type": "text",
                    "value": "maxlength"
                  }
                ]
              },
              {
                "type": "text",
                "value": ", otherwise the value will never be valid, as it is impossible to meet both criteria."
              }
            ]
          }
        ],
