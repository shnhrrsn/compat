  "content": {
    "intro": {
      "id": "2b628a0c72b393bb52b89e3cd25cf55e",
      "tree": {
        "type": "root",
        "children": [
          {
            "type": "element",
            "tagName": "p",
            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "decoding"
                  }
                ]
              },
              {
                "type": "text",
                "value": " attribute, valid on "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/svg/elements/image"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "<image>"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " elements, provides a hint to the browser as to whether it should perform image decoding along with rendering other content in a single presentation step that looks more \"correct\" ("
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "sync"
                  }
                ]
              },
              {
                "type": "text",
                "value": "), or render and present the other content first and then decode the image and present it later ("
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "async"
                  }
                ]
              },
              {
                "type": "text",
                "value": "). In practice, "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "async"
                  }
                ]
              },
              {
                "type": "text",
                "value": " means that the next paint does not wait for the image to decode."
              }
            ]
          }
        ],
        "data": {
          "quirksMode": false
        }
      }
    },
