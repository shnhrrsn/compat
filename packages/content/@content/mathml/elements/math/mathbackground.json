            "children": [
              {
                "type": "text",
                "value": "The top-level element in MathML is "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "<math>"
                  }
                ]
              },
              {
                "type": "text",
                "value": ". Every valid MathML instance must be wrapped in "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "<math>"
                  }
                ]
              },
              {
                "type": "text",
                "value": " tags. In addition you must not nest a second "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "<math>"
                  }
                ]
              },
              {
                "type": "text",
                "value": " element in another, but you can have an arbitrary number of other child elements in it."
              }
            ]
          }
