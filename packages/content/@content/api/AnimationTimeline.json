            "children": [
              {
                "type": "text",
                "value": "The "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "AnimationTimeline"
                  }
                ]
              },
              {
                "type": "text",
                "value": " interface of the "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "href": "/en-US/docs/Web/API/Web_Animations_API"
                },
                "children": [
                  {
                    "type": "text",
                    "value": "Web Animations API"
                  }
                ]
              },
              {
                "type": "text",
                "value": " represents the timeline of an animation. This interface exists to define timeline features (inherited by "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/DocumentTimeline"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "DocumentTimeline"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " and future timeline types) and is not itself directly used by developers. Anywhere you see "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "AnimationTimeline"
                  }
                ]
              },
              {
                "type": "text",
                "value": ", you should use "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "DocumentTimeline"
                  }
                ]
              },
              {
                "type": "text",
                "value": " or any other timeline type instead."
              }
            ]
          }
