              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "\"video\""
                  }
                ]
              },
              {
                "type": "text",
                "value": " if it is a video track.\nIt doesn't change if the track is disassociated from its source."
              }
