            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The "
              },
              {
                "type": "element",
                "tagName": "strong",
                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "MediaStreamTrack.kind"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": "\nread-only property returns a string set to "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "\"audio\""
                  }
                ]
              },
              {
                "type": "text",
                "value": " if\nthe track is an audio track and to "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "\"video\""
                  }
                ]
              },
              {
                "type": "text",
                "value": ", if it is a video\ntrack. It doesn't change if the track is disassociated from its source."
              }
            ]
          }
        ],
