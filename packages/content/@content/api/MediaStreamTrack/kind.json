                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "MediaStreamTrack.kind"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " read-only property returns a string set to "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "\"audio\""
                  }
                ]
              },
              {
                "type": "text",
                "value": " if the track is an audio track and to "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "\"video\""
                  }
                ]
              },
              {
                "type": "text",
                "value": ", if it is a video track.\nIt doesn't change if the track is disassociated from its source."
              }
            ]
          }
        ],
