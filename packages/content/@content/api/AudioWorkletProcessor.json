            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The "
              },
              {
                "type": "element",
                "tagName": "strong",
                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "AudioWorkletProcessor"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " interface of the "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "href": "/en-US/docs/Web/API/Web_Audio_API"
                },
                "children": [
                  {
                    "type": "text",
                    "value": "Web Audio API"
                  }
                ]
              },
              {
                "type": "text",
                "value": " represents an audio processing code behind a custom "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/AudioWorkletNode"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "AudioWorkletNode"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": ". It lives in the "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/AudioWorkletGlobalScope"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "AudioWorkletGlobalScope"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " and runs on the Web Audio rendering thread. In turn, an "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/AudioWorkletNode"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "AudioWorkletNode"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " based on it runs on the main thread."
              }
            ]
          }
        ],
