      "tree": {
        "type": "root",
        "children": [
          {
            "type": "element",
            "tagName": "p",
            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The DOM "
              },
              {
                "type": "element",
                "tagName": "strong",
                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "beforeinput"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " event fires when the value of an "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/html/elements/input"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "<input>"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " or "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/html/elements/textarea"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "<textarea>"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " element is about to be modified. But in contrast to the "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "input"
                  }
                ]
              },
              {
                "type": "text",
                "value": " event, it does not fire on the "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/html/elements/select"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "<select>"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " element. The event also applies to elements with "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/HTMLElement/contentEditable"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "contenteditable"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " enabled, and to any element when "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/Document/designMode"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "designMode"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " is turned on."
              }
            ]
          }
        ],
