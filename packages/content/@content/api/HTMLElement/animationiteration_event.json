                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "animationend"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " event, and therefore does not occur for animations with an "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "animation-iteration-count"
                  }
                ]
              },
              {
                "type": "text",
                "value": " of one."
              }
