      "history": [
        {
          "notes": [
            "<code>enumerateDevices()</code> enumerates both input and output devices. Previously only input devices were returned."
          ],
          "added": {
            "version": "preview",
            "date": null
          },
          "removed": null,
          "usage": {
            "global": null,
            "relative": null
          }
        },
        {
          "notes": [
            "<code>enumerateDevices()</code> only returns input devices."
          ],
          "added": {
            "version": "39.0.0",
            "date": 1435795200
          },
          "removed": null,
          "usage": {
            "global": 0.025499999999999988,
            "relative": 1
          },
          "partial_implementation": true
        },
        {
          "notes": [
            "Before Firefox 63, <code>enumerateDevices()</code> only returned input devices. Starting with Firefox 63, output devices are also included if the <code>media.setsinkid.enabled</code> preference is enabled."
          ],
          "added": {
            "version": "63.0.0",
            "date": 1540252800
          },
          "removed": null,
