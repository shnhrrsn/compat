            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The "
              },
              {
                "type": "element",
                "tagName": "strong",
                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "movementY"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " read-only property of the "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/MouseEvent"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "MouseEvent"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " interface provides the difference in the Y coordinate of the mouse pointer between the given event and the previous "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/Element/mousemove_event"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "mousemove"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " event.\nIn other words, the value of the property is computed like this: "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "currentEvent.movementY = currentEvent.screenY - previousEvent.screenY"
                  }
                ]
              },
              {
                "type": "text",
                "value": "."
              }
            ]
          }
