  "content": {
    "intro": {
      "id": "634d976964227c05651dc6463df72a5d",
      "tree": {
        "type": "root",
        "children": [
          {
            "type": "element",
            "tagName": "p",
            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "A website can indicate that a particular "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "href": "/en-US/docs/Web/HTTP/Client_hints"
                },
                "children": [
                  {
                    "type": "text",
                    "value": "Client Hint"
                  }
                ]
              },
              {
                "type": "text",
                "value": " is critical to the page by including it in a "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/http/headers/Critical-CH"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "Critical-CH"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " HTTP response header (as well as the "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/http/headers/Accept-CH"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "Accept-CH"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " HTTP request header which is needed for all client hints whether critical or not). Doing so will trigger a connection restart if the hint listed in the "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "Critical-CH"
                  }
                ]
              },
              {
                "type": "text",
                "value": " HTTP response header could have been, but wasn't, included in the HTTP request initially sent. If the browser does not support that client hint, it is ignored and no connection restart occurs."
              }
            ]
          }
        ],
        "data": {
