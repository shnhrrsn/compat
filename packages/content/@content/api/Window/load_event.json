  "content": {
    "intro": {
      "id": "1672515a1676e27bfbd6b7633ee44e48",
      "tree": {
        "type": "root",
        "children": [
          {
            "type": "element",
            "tagName": "p",
            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The "
              },
              {
                "type": "element",
                "tagName": "strong",
                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "load"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " event is fired when the whole page has loaded, including all dependent resources such as stylesheets and images. This is in contrast to "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/Document/DOMContentLoaded_event"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "DOMContentLoaded"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": ", which is fired as soon as the page DOM has been loaded, without waiting for resources to finish loading."
              }
            ]
          }
        ],
        "data": {
          "quirksMode": false
        }
      }
    },
