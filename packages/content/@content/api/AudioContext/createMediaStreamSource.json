            "children": [
              {
                "type": "text",
                "value": "The "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "createMediaStreamSource()"
                  }
                ]
              },
              {
                "type": "text",
                "value": " method of the "
              },
              {
                "type": "element",
