        "children": [
          {
            "type": "element",
            "tagName": "p",
            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "createMediaStreamSource()"
                  }
                ]
              },
              {
                "type": "text",
                "value": " method of the "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/AudioContext"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "AudioContext"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": "\nInterface is used to create a new "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/MediaStreamAudioSourceNode"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "MediaStreamAudioSourceNode"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": "\nobject, given a media stream (say, from a "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/MediaDevices/getUserMedia"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "MediaDevices.getUserMedia"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": "\ninstance), the audio from which can then be played and manipulated."
              }
            ]
          }
        ],
