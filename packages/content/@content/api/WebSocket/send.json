  "content": {
    "intro": {
      "id": "d2fd12cecbc129cca8fb7bd6bd36d0e8",
      "tree": {
        "type": "root",
        "children": [
          {
            "type": "element",
            "tagName": "p",
            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The "
              },
              {
                "type": "element",
                "tagName": "strong",
                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "WebSocket.send()"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " method enqueues the specified data\nto be transmitted to the server over the WebSocket connection, increasing the value of\n"
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "bufferedAmount"
                  }
                ]
              },
              {
                "type": "text",
                "value": " by the number of bytes needed to contain the data. If the\ndata can't be sent (for example, because it needs to be buffered but the buffer is\nfull), the socket is closed automatically.\nThe browser will throw an exception if you call "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "send()"
                  }
                ]
              },
              {
                "type": "text",
                "value": " when the connection is in the "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "CONNECTING"
                  }
                ]
              },
              {
                "type": "text",
                "value": " state. If you call "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "send()"
                  }
                ]
              },
              {
                "type": "text",
                "value": " when the connection is in the "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "CLOSING"
                  }
                ]
              },
              {
                "type": "text",
                "value": " or "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "CLOSED"
                  }
                ]
              },
              {
                "type": "text",
                "value": " states, the browser will silently discard the data."
              }
            ]
          }
        ],
        "data": {
          "quirksMode": false
        }
      }
    }
  },
