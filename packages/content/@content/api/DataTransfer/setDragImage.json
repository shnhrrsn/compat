    "intro": {
      "id": "f415385f6e8190f24239db1f0f110b9a",
      "tree": {
        "type": "root",
        "children": [
          {
            "type": "element",
            "tagName": "p",
            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "When a drag occurs, a translucent image is generated from the drag target (the element\nthe "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/HTMLElement/dragstart_event"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "dragstart"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " event is fired at), and follows the mouse pointer during the\ndrag. This image is created automatically, so you do not need to create it yourself.\nHowever, if a custom image is desired, the\n"
              },
              {
                "type": "element",
                "tagName": "strong",
                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "DataTransfer.setDragImage()"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " method can be used to set the\ncustom image to be used. The image will typically be an "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/html/elements/img"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "<img>"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " element\nbut it can also be a "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/html/elements/canvas"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "<canvas>"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " or any other visible element."
              }
            ]
          }
        ],
