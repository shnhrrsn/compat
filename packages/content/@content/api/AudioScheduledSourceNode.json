            "children": [
              {
                "type": "text",
                "value": "The "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "AudioScheduledSourceNode"
                  }
                ]
              },
              {
                "type": "text",
                "value": " interface—part of the Web Audio API—is a parent interface for several types of audio source node interfaces which share the ability to be started and stopped, optionally at specified times. Specifically, this interface defines the "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/AudioScheduledSourceNode/start"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "start()"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " and "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/AudioScheduledSourceNode/stop"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "stop()"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " methods, as well as the "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/AudioScheduledSourceNode/ended_event"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "ended"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " event."
              }
            ]
          }
