                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "PerformanceTiming"
                  }
                ]
              },
              {
                "type": "text",
                "value": " object describing your page using the "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/Performance/timing"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "window.performance.timing"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " property."
              }
            ]
          }
        ],
