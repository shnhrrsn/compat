  "content": {
    "intro": {
      "id": "5c74f5ad8cfcdc67c2612dbd90eec901",
      "tree": {
        "type": "root",
        "children": [
          {
            "type": "element",
            "tagName": "p",
            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "connect()"
                  }
                ]
              },
              {
                "type": "text",
                "value": " method of the "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/AudioNode"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "AudioNode"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " interface lets\nyou connect one of the node's outputs to a target, which may be either another\n"
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "AudioNode"
                  }
                ]
              },
              {
                "type": "text",
                "value": " (thereby directing the sound data to the specified node) or an\n"
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/AudioParam"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "AudioParam"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": ", so that the node's output data is automatically used to\nchange the value of that parameter over time."
              }
            ]
          }
        ],
