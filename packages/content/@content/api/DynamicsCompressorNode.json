            "children": [
              {
                "type": "text",
                "value": "The "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "DynamicsCompressorNode"
                  }
                ]
              },
              {
                "type": "text",
                "value": " interface provides a compression effect, which lowers the volume of the loudest parts of the signal in order to help prevent clipping and distortion that can occur when multiple sounds are played and multiplexed together at once. This is often used in musical production and game audio. "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "DynamicsCompressorNode"
                  }
                ]
              },
              {
                "type": "text",
                "value": " is an "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/AudioNode"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "AudioNode"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " that has exactly one input and one output."
              }
            ]
          }
        ],
