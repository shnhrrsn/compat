                    "children": [
                      {
                        "type": "element",
                        "tagName": "code",
                        "properties": {},
                        "children": [
                          {
                            "type": "text",
                            "value": "stopImmediatePropagation()"
                          }
                        ]
                      }
                    ]
                  },
                  {
                    "type": "text",
                    "value": " to not call any further listeners for the same event at the same level in the DOM"
                  }
                ]
              },
              {
                "type": "text",
                "value": "\n"
              },
              {
                "type": "element",
                "tagName": "li",
                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "a",
                    "properties": {
