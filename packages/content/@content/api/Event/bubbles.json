    "seeAlso": {
      "id": "f8de428e15980b0fd0c92c55a23e5532",
      "tree": {
        "type": "root",
        "children": [
          {
            "type": "element",
            "tagName": "ul",
            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "\n"
              },
              {
                "type": "element",
                "tagName": "li",
                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "a",
                    "properties": {
                      "internal": "",
                      "href": "/api/Event/stopPropagation"
                    },
                    "children": [
                      {
                        "type": "element",
                        "tagName": "code",
                        "properties": {},
                        "children": [
                          {
                            "type": "text",
                            "value": "stopPropagation()"
                          }
                        ]
                      }
                    ]
                  },
                  {
                    "type": "text",
                    "value": " to prevent further propagation of the current event in the capturing and bubbling phases"
                  }
                ]
              },
              {
                "type": "text",
                "value": "\n"
              },
              {
                "type": "element",
                "tagName": "li",
                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "a",
                    "properties": {
                      "internal": "",
                      "href": "/api/Event/stopImmediatePropagation"
                    },
                    "children": [
                      {
                        "type": "element",
                        "tagName": "code",
                        "properties": {},
                        "children": [
                          {
                            "type": "text",
                            "value": "stopImmediatePropagation()"
                          }
                        ]
                      }
                    ]
                  },
                  {
                    "type": "text",
                    "value": " to not call any further listeners for the same event at the same level in the DOM"
                  }
                ]
              },
              {
                "type": "text",
                "value": "\n"
              },
              {
                "type": "element",
                "tagName": "li",
                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "a",
                    "properties": {
                      "internal": "",
                      "href": "/api/Event/preventDefault"
                    },
                    "children": [
                      {
                        "type": "element",
                        "tagName": "code",
                        "properties": {},
                        "children": [
                          {
                            "type": "text",
                            "value": "preventDefault()"
                          }
                        ]
                      }
                    ]
                  },
                  {
                    "type": "text",
                    "value": " to allow propagation to continue but to disallow the browser to perform its default action should no listeners handle the event"
                  }
                ]
              },
              {
                "type": "text",
                "value": "\n"
              }
            ]
