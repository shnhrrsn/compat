        "children": [
          {
            "type": "element",
            "tagName": "p",
            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "Returns the online status of the browser. The property returns a boolean value, with "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "true"
                  }
                ]
              },
              {
                "type": "text",
                "value": " meaning online and "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "false"
                  }
                ]
              },
              {
                "type": "text",
                "value": " meaning offline. The property sends updates whenever the browser's ability to connect to the network changes. The update occurs when the user follows links or when a script requests a remote page."
              }
            ]
          }
        ],
        "data": {
          "quirksMode": false
        }
      }
    }
