                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "SVGGlyphRefElement"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " property. If the\nfont is in one of the formats listed in "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "href": "https://www.w3.org/TR/2008/REC-CSS2-20080411/fonts.html#referencing"
                },
                "children": [
                  {
                    "type": "text",
                    "value": "CSS2([CSS2],\nsection15.3.5)"
                  }
                ]
              },
              {
                "type": "text",
                "value": ", then its value is the corresponding <string> parameter\nof the font."
              }
            ]
          }
        ],
