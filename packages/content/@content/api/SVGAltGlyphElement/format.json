    "intro": {
      "id": "e70df27e9f13a0b3e5a627ca28376134",
      "tree": {
        "type": "root",
        "children": [
          {
            "type": "element",
            "tagName": "p",
            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The "
              },
              {
                "type": "element",
                "tagName": "strong",
                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "SVGAltGlyphElement.format"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " property is a\nstring that defines the format of the given font. It has the same\nmeaning as the 'format' property of "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/SVGGlyphRefElement"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "SVGGlyphRefElement"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " property. If the\nfont is in one of the formats listed in "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "href": "https://www.w3.org/TR/2008/REC-CSS2-20080411/fonts.html#referencing"
                },
                "children": [
                  {
                    "type": "text",
                    "value": "CSS2 section 15.3.5"
                  }
                ]
              },
              {
                "type": "text",
                "value": ", then its value is the corresponding <string> parameter of the font."
              }
            ]
          }
        ],
