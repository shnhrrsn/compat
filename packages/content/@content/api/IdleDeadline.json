  "content": {
    "intro": {
      "id": "ef6e145627f8a034b296e3aba3e3316f",
      "tree": {
        "type": "root",
        "children": [
          {
            "type": "element",
            "tagName": "p",
            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "IdleDeadline"
                  }
                ]
              },
              {
                "type": "text",
                "value": " interface is used as the data type of the input parameter to idle callbacks established by calling "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/Window/requestIdleCallback"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "Window.requestIdleCallback()"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": ". It offers a method, "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/IdleDeadline/timeRemaining"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "timeRemaining()"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": ", which lets you determine how much longer the user agent estimates it will remain idle and a property, "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/IdleDeadline/didTimeout"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "didTimeout"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": ", which lets you determine if your callback is executing because its timeout duration expired."
              }
            ]
          }
        ],
        "data": {
