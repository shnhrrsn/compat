  "content": {
    "intro": {
      "id": "97b6dba8039aac4eafc7f6f2514035f0",
      "tree": {
        "type": "root",
        "children": [
          {
            "type": "element",
            "tagName": "p",
            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The "
              },
              {
                "type": "element",
                "tagName": "strong",
                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "ExtendableEvent.waitUntil()"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": "\nmethod tells the event dispatcher that work is ongoing. It can also be used to detect\nwhether that work was successful. In service workers, "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "waitUntil()"
                  }
                ]
              },
              {
                "type": "text",
                "value": " tells\nthe browser that work is ongoing until the promise settles, and it shouldn't terminate\nthe service worker if it wants that work to complete."
              }
            ]
          }
        ],
        "data": {
          "quirksMode": false
        }
      }
