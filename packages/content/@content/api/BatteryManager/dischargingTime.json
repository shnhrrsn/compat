                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "dischargingtimechange"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " event is fired."
              }
            ]
          }
        ],
