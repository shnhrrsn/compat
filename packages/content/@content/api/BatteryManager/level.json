  "content": {
    "intro": {
      "id": "1c86a0d6a216fef3e8f8c2322f247f0e",
      "tree": {
        "type": "root",
        "children": [
          {
            "type": "element",
            "tagName": "p",
            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The "
              },
              {
                "type": "element",
                "tagName": "strong",
                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "BatteryManager.level"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " property indicates the current battery charge level as a value between "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "0.0"
                  }
                ]
              },
              {
                "type": "text",
                "value": " and "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "1.0"
                  }
                ]
              },
              {
                "type": "text",
                "value": ".\nA value of "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "0.0"
                  }
                ]
              },
              {
                "type": "text",
                "value": " means the battery is empty and the system is about to be suspended.\nA value of "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "1.0"
                  }
                ]
              },
              {
                "type": "text",
                "value": " means the battery is full.\nA value of "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "1.0"
                  }
                ]
              },
              {
                "type": "text",
                "value": " is also returned if the implementation isn't able to determine the battery charge level\nor if the system is not battery-powered.\nWhen its value changes, the "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "href": "/en-US/docs/Web/API/BatteryManager/levelchange_event"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "levelchange"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " event is fired."
              }
            ]
          }
        ],
