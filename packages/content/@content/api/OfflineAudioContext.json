        "children": [
          {
            "type": "element",
            "tagName": "p",
            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "OfflineAudioContext"
                  }
                ]
              },
              {
                "type": "text",
                "value": " interface is an "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/AudioContext"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "AudioContext"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " interface representing an audio-processing graph built from linked together "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/AudioNode"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "AudioNode"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": "s. In contrast with a standard "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/AudioContext"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "AudioContext"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": ", an "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "OfflineAudioContext"
                  }
                ]
              },
              {
                "type": "text",
                "value": " doesn't render the audio to the device hardware; instead, it generates it, as fast as it can, and outputs the result to an "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/AudioBuffer"
                },
                "children": [
