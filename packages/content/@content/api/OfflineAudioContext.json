            "children": [
              {
                "type": "text",
                "value": "The "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "OfflineAudioContext"
                  }
                ]
              },
              {
                "type": "text",
                "value": " interface is an "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/AudioContext"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
