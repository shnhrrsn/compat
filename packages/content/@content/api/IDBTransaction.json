  "content": {
    "intro": {
      "id": "36c81738cc5f193acd7802601cc783df",
      "tree": {
        "type": "root",
        "children": [
          {
            "type": "element",
            "tagName": "p",
            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The "
              },
              {
                "type": "element",
                "tagName": "strong",
                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "IDBTransaction"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " interface of the "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "href": "/en-US/docs/Web/API/IndexedDB_API"
                },
                "children": [
                  {
                    "type": "text",
                    "value": "IndexedDB API"
                  }
                ]
              },
              {
                "type": "text",
                "value": " provides a static, asynchronous transaction on a database using event handler attributes. All reading and writing of data is done within transactions. You use "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/IDBDatabase"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "IDBDatabase"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " to start transactions, "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/IDBTransaction"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "IDBTransaction"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " to set the mode of the transaction (e.g. is it "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "readonly"
                  }
                ]
              },
              {
                "type": "text",
                "value": " or "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "readwrite"
                  }
                ]
              },
              {
                "type": "text",
                "value": "), and you access an "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/IDBObjectStore"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "IDBObjectStore"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " to make a request. You can also use an "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "IDBTransaction"
                  }
                ]
              },
              {
                "type": "text",
                "value": " object to abort transactions."
              }
            ]
          }
        ],
        "data": {
          "quirksMode": false
        }
      }
    },
