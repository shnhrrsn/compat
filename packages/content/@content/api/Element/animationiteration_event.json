                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "animationiteration"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " event is fired when an iteration of a "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "href": "/en-US/docs/Web/CSS/CSS_animations"
                },
                "children": [
                  {
                    "type": "text",
                    "value": "CSS Animation"
                  }
                ]
              },
              {
                "type": "text",
                "value": " ends, and another one begins. This event does not occur at the same time as the "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/Element/animationend_event"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "animationend"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " event, and therefore does not occur for animations with an "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "animation-iteration-count"
                  }
                ]
              },
              {
                "type": "text",
                "value": " of one."
              }
            ]
          }
