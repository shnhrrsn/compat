            "children": [
              {
                "type": "text",
                "value": "The "
              },
              {
                "type": "element",
                "tagName": "strong",
                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "XRRenderState"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " interface of the "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "href": "/en-US/docs/Web/API/WebXR_Device_API"
                },
                "children": [
                  {
                    "type": "text",
                    "value": "WebXR Device API"
                  }
                ]
              },
              {
                "type": "text",
                "value": " contains configurable values which affect how the imagery generated by an "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/XRSession"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "XRSession"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " gets composited. These properties include the range of distances from the viewer within which content should be rendered, the vertical field of view (for inline presentations), and a reference to the "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/XRWebGLLayer"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "XRWebGLLayer"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " being used as the target for rendering the scene prior to it being presented on the XR device's display or displays."
              }
            ]
          }
        ],
