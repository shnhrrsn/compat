        "children": [
          {
            "type": "element",
            "tagName": "p",
            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The read-only "
              },
              {
                "type": "element",
                "tagName": "strong",
                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "XMLHttpRequest.statusText"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " property returns a string containing the response's status message as returned by the HTTP server. Unlike "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "href": "/en-US/docs/Web/API/XMLHttpRequest/status"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "XMLHTTPRequest.status"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " which indicates a numerical status code, this property contains the "
              },
              {
                "type": "element",
                "tagName": "em",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "text"
                  }
                ]
              },
              {
                "type": "text",
                "value": " of the response status, such as \"OK\" or \"Not Found\". If the request's "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "href": "/en-US/docs/Web/API/XMLHttpRequest/readyState"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "readyState"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " is in "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "UNSENT"
                  }
                ]
              },
              {
                "type": "text",
                "value": " or "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "OPENED"
                  }
                ]
              },
              {
                "type": "text",
                "value": " state, the value of "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "statusText"
                  }
                ]
              },
              {
                "type": "text",
                "value": " will be an empty string."
              }
            ]
          }
