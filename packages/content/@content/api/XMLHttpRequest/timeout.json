                "children": [
                  {
                    "type": "text",
                    "value": "document environment"
                  }
                ]
              },
              {
                "type": "text",
                "value": " or it will throw an "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "InvalidAccessError"
                  }
                ]
              },
              {
                "type": "text",
                "value": " exception. When a timeout happens, a "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "href": "/en-US/docs/Web/API/XMLHttpRequest/timeout_event"
                },
                "children": [
                  {
                    "type": "text",
                    "value": "timeout"
                  }
                ]
              },
              {
                "type": "text",
                "value": " event is fired."
              }
            ]
          }
