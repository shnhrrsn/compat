              {
                "type": "text",
                "value": " and "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "href": "/en-US/docs/Web/HTML/Element/script#module"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
