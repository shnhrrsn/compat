      "tree": {
        "type": "root",
        "children": [
          {
            "type": "element",
            "tagName": "p",
            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The "
              },
              {
                "type": "element",
                "tagName": "strong",
                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "MediaTrackSettings"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " dictionary is used to return the current values configured for each of a "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/MediaStreamTrack"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "MediaStreamTrack"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": "'s settings. These values will adhere as closely as possible to any constraints previously described using a "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/MediaTrackConstraints"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "MediaTrackConstraints"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " object and set using "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/MediaStreamTrack/applyConstraints"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "applyConstraints()"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": ", and will adhere to the default constraints for any properties whose constraints haven't been changed, or whose customized constraints couldn't be matched."
              }
            ]
          }
        ],
        "data": {
