      "tree": {
        "type": "root",
        "children": [
          {
            "type": "element",
            "tagName": "p",
            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The read-only property\n"
              },
              {
                "type": "element",
                "tagName": "strong",
                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "RTCPeerConnection.pendingRemoteDescription"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " returns an\n"
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/RTCSessionDescription"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "RTCSessionDescription"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " object describing a pending configuration change\nfor the remote end of the connection. This does not describe the connection as it\ncurrently stands, but as it may exist in the near future. Use\n"
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/RTCPeerConnection/currentRemoteDescription"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "RTCPeerConnection.currentRemoteDescription"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " or\n"
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/RTCPeerConnection/remoteDescription"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "RTCPeerConnection.remoteDescription"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " to get the current session\ndescription for the remote endpoint. For details on the difference, see\n{{SectionOnPage(\"/en-US/docs/Web/API/WebRTC_API/Connectivity\", \"Pending and current\ndescriptions\")}}."
              }
            ]
          }
        ],
        "data": {
          "quirksMode": false
        }
      }
    },
