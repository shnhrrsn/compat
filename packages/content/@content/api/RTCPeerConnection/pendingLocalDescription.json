      "tree": {
        "type": "root",
        "children": [
          {
            "type": "element",
            "tagName": "p",
            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The read-only property\n"
              },
              {
                "type": "element",
                "tagName": "strong",
                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "RTCPeerConnection.pendingLocalDescription"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " returns an\n"
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/RTCSessionDescription"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "RTCSessionDescription"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " object describing a pending configuration change\nfor the local end of the connection. This does not describe the connection as it\ncurrently stands, but as it may exist in the near future. Use\n"
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/RTCPeerConnection/currentLocalDescription"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "RTCPeerConnection.currentLocalDescription"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " or\n"
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/RTCPeerConnection/localDescription"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "RTCPeerConnection.localDescription"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " to get the current state of the\nendpoint. For details on the difference, see\n"
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "href": "/en-US/docs/Web/API/WebRTC_API/Connectivity#pending_and_current_descriptions"
                },
                "children": [
                  {
                    "type": "text",
                    "value": "Pending and current descriptions"
                  }
                ]
              },
              {
                "type": "text",
                "value": " in the WebRTC Connectivity page."
              }
            ]
          }
        ],
