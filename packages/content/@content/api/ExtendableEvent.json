            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The "
              },
              {
                "type": "element",
                "tagName": "strong",
                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "ExtendableEvent"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " interface extends the lifetime of the "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "href": "/en-US/docs/Web/API/ServiceWorkerGlobalScope/install_event"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "install"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " and "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "href": "/en-US/docs/Web/API/ServiceWorkerGlobalScope/activate_event"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "activate"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " events dispatched on the global scope as part of the service worker lifecycle. This ensures that any functional events (like "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/FetchEvent"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "FetchEvent"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": ") are not dispatched until it upgrades database schemas and deletes the outdated cache entries."
              }
            ]
          }
        ],
