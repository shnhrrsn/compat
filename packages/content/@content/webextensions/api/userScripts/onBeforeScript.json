        "children": [
          {
            "type": "element",
            "tagName": "p",
            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "onBeforeScript"
                  }
                ]
              },
              {
                "type": "text",
                "value": " event of the "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/webextensions/api/userScripts"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "browser.userScripts"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " is fired before a user script is executed. It can only be included in the API script, the script registered in "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "href": "/en-US/docs/Mozilla/Add-ons/WebExtensions/manifest.json/user_scripts"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "\"user_scripts\""
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": ", where it is used to detect that the custom API methods should be exported to the user script."
              }
            ]
          }
        ],
