  "content": {
    "intro": {
      "id": "c1390863da5f96dbd355f2455903e301",
      "tree": {
        "type": "root",
        "children": [
          {
            "type": "element",
            "tagName": "p",
            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "Updates the extension's set of static rulesets. The rulesets with IDs listed in "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "options.disableRulesetIds"
                  }
                ]
              },
              {
                "type": "text",
                "value": " are first deactivated, and then the rulesets listed in "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "options.enableRulesetIds"
                  }
                ]
              },
              {
                "type": "text",
                "value": " are activated. Note that the set of enabled static rulesets persists across sessions but not across extension updates, i.e. the "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "href": "/docs/Mozilla/Add-ons/WebExtensions/manifest.json/declarative_net_request"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "declarative_net_request.rule_resources"
                      }
                    ]
                  },
                  {
                    "type": "text",
                    "value": " manifest key"
                  }
                ]
              },
              {
                "type": "text",
                "value": " determines the set of enabled static rulesets on each extension update."
              }
            ]
          }
        ],
        "data": {
          "quirksMode": false
        }
      }
    }
  },
