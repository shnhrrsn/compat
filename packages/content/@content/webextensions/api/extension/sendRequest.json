            "children": [
              {
                "type": "text",
                "value": "Sends a request to other listeners within the extension. Similar to "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/webextensions/api/runtime/connect"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "runtime.connect"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": ", but only sends a request with an optional response. The "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/webextensions/api/extension/onRequest"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "extension.onRequest"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " event fires in each page of the extension."
              }
            ]
          }
