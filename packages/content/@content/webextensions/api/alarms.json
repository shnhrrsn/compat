                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "setInterval()"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": ", except that those functions don't work with background pages that are loaded on demand."
              }
            ]
