                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "runtime.connect()"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " and "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/webextensions/api/runtime/sendMessage"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "runtime.sendMessage()"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " message passing. If "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "externally_connectable"
                  }
                ]
              },
              {
                "type": "text",
                "value": " is not specified, all extensions can communicate with each other but not with web pages."
              }
            ]
          }
        ],
