                ]
              },
              {
                "type": "text",
                "value": " property sets the horizontal spacing behavior between text characters. This value is added to the natural spacing between characters while rendering the text. Positive values of "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "letter-spacing"
                  }
                ]
              },
              {
                "type": "text",
                "value": " causes characters to spread farther apart, while negative values of "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "letter-spacing"
                  }
                ]
              },
              {
                "type": "text",
                "value": " bring characters closer together."
              }
            ]
