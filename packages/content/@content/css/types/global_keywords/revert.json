                      }
                    ]
                  },
                  {
                    "type": "text",
                    "value": " keyword to make an element's property the same as its parent."
                  }
                ]
              },
              {
                "type": "text",
                "value": "\n"
              },
              {
                "type": "element",
                "tagName": "li",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "Use the "
                  },
                  {
                    "type": "element",
                    "tagName": "a",
                    "properties": {
                      "internal": "",
                      "href": "/css/types/global_keywords/revert-layer"
                    },
                    "children": [
                      {
                        "type": "element",
                        "tagName": "code",
                        "properties": {},
                        "children": [
                          {
                            "type": "text",
                            "value": "revert-layer"
                          }
                        ]
                      }
                    ]
                  },
                  {
                    "type": "text",
                    "value": " keyword to reset a property to the value established in a previous cascade layer."
                  }
                ]
              },
              {
                "type": "text",
                "value": "\n"
              },
              {
                "type": "element",
                "tagName": "li",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "Use the "
                  },
                  {
                    "type": "element",
                    "tagName": "a",
                    "properties": {
                      "internal": "",
