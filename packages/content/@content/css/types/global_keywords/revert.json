                      }
                    ]
                  },
                  {
                    "type": "text",
                    "value": " to set a property to its initial value."
                  }
                ]
              },
              {
                "type": "text",
                "value": "\n"
              },
              {
                "type": "element",
                "tagName": "li",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "Use "
                  },
                  {
                    "type": "element",
                    "tagName": "a",
                    "properties": {
                      "internal": "",
                      "href": "/css/types/global_keywords/unset"
                    },
                    "children": [
                      {
                        "type": "element",
                        "tagName": "code",
                        "properties": {},
                        "children": [
                          {
                            "type": "text",
                            "value": "unset"
                          }
                        ]
                      }
                    ]
                  },
                  {
                    "type": "text",
                    "value": " to set a property to its inherited value if it inherits, or to its initial value if not."
                  }
                ]
              },
              {
                "type": "text",
                "value": "\n"
              },
              {
                "type": "element",
                "tagName": "li",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "Use "
                  },
                  {
                    "type": "element",
                    "tagName": "a",
                    "properties": {
                      "internal": "",
                      "href": "/css/properties/cursor/inherit"
                    },
                    "children": [
                      {
                        "type": "element",
                        "tagName": "code",
                        "properties": {},
                        "children": [
                          {
                            "type": "text",
                            "value": "inherit"
                          }
                        ]
                      }
                    ]
                  },
                  {
                    "type": "text",
                    "value": " to make an element's property the same as its parent."
                  }
                ]
              },
              {
                "type": "text",
                "value": "\n"
              },
              {
                "type": "element",
                "tagName": "li",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "The "
                  },
                  {
                    "type": "element",
                    "tagName": "a",
                    "properties": {
                      "internal": "",
                      "href": "/css/properties/all"
                    },
                    "children": [
                      {
                        "type": "element",
                        "tagName": "code",
                        "properties": {},
                        "children": [
                          {
                            "type": "text",
                            "value": "all"
                          }
                        ]
                      }
                    ]
                  },
                  {
                    "type": "text",
                    "value": " property lets you reset all properties to their initial, inherited, reverted, or unset state at once."
                  }
