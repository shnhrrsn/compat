                  {
                    "type": "text",
                    "value": "Use the "
                  },
                  {
                    "type": "element",
                    "tagName": "a",
                    "properties": {
                      "internal": "",
                      "href": "/css/types/global_keywords/revert-layer"
                    },
                    "children": [
                      {
                        "type": "element",
                        "tagName": "code",
                        "properties": {},
