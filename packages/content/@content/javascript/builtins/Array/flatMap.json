                    "children": [
                      {
                        "type": "text",
                        "value": "map()"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " followed by a "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/javascript/builtins/Array/flat"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "flat()"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " of depth 1 ("
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "arr.map(...args).flat()"
                  }
                ]
              },
              {
                "type": "text",
                "value": "), but slightly more efficient than calling those two methods separately."
              }
            ]
          }
