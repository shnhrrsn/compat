                "type": "element",
                "tagName": "a",
                "properties": {
                  "href": "/en-US/docs/Web/JavaScript/Inheritance_and_the_prototype_chain"
                },
                "children": [
                  {
                    "type": "text",
                    "value": "prototypes"
                  }
                ]
              },
              {
                "type": "text",
                "value": " but also have some syntax and semantics that are unique to classes."
              }
            ]
          }
