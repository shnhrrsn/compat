                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/javascript/builtins/Math/pow"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "Math.pow()"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": ", except it also accepts "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "href": "/en-US/docs/Web/JavaScript/Reference/Global_Objects/BigInt"
                },
                "children": [
                  {
                    "type": "text",
                    "value": "BigInts"
                  }
                ]
              },
              {
                "type": "text",
                "value": " as operands."
              }
            ]
          }
