            "children": [
              {
                "type": "text",
                "value": "A "
              },
              {
                "type": "element",
                "tagName": "strong",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "non-capturing group"
                  }
                ]
              },
              {
                "type": "text",
                "value": " groups a subpattern, allowing you to apply a "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "href": "/en-US/docs/Web/JavaScript/Reference/Regular_expressions/Quantifier"
                },
