      "tree": {
        "type": "root",
        "children": [
          {
            "type": "element",
            "tagName": "p",
            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The static "
              },
              {
                "type": "element",
                "tagName": "strong",
                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "import"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " declaration is used to import read-only live bindings which are "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "href": "/en-US/docs/Web/JavaScript/Reference/Statements/export"
                },
                "children": [
                  {
                    "type": "text",
                    "value": "exported"
                  }
                ]
              },
              {
                "type": "text",
                "value": " by another module. The imported bindings are called "
              },
              {
                "type": "element",
                "tagName": "em",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "live bindings"
                  }
                ]
              },
              {
                "type": "text",
                "value": " because they are updated by the module that exported the binding, but cannot be modified by the importing module."
              }
            ]
          }
        ],
        "data": {
          "quirksMode": false
        }
      }
    },
