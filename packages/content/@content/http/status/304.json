  "content": {
    "intro": {
      "id": "b7f2d5f7cf2658179a64b73f130f5761",
      "tree": {
        "type": "root",
        "children": [
          {
            "type": "element",
            "tagName": "p",
            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The HTTP "
              },
              {
                "type": "element",
                "tagName": "strong",
                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "304 Not Modified"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " client redirection response code indicates that there is no need to retransmit the requested resources.\nThis response code is sent when the request is a conditional "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/http/methods/GET"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "GET"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " or "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/http/methods/HEAD"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "HEAD"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " request with an "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/http/headers/If-None-Match"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "If-None-Match"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " or an "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/http/headers/If-Modified-Since"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "If-Modified-Since"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " header and the condition evaluates to false.\nIt is an implicit redirection to a cached resource that would have resulted in a "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/http/status/200"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "200"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "OK"
                  }
                ]
              },
              {
                "type": "text",
                "value": " response if the condition evaluated to true."
              }
            ]
          }
        ],
