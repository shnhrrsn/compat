  "content": {
    "intro": {
      "id": "9080e60b15b1b6dca67424010bc3f0e7",
      "tree": {
        "type": "root",
        "children": [
          {
            "type": "element",
            "tagName": "p",
            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The HyperText Transfer Protocol (HTTP) "
              },
              {
                "type": "element",
                "tagName": "strong",
                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "301 Moved Permanently"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " redirect status response code indicates that the requested resource has been definitively moved to the URL given by the "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/http/headers/Location"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "Location"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " headers. A browser redirects to the new URL and search engines update their links to the resource."
              }
            ]
          }
        ],
        "data": {
          "quirksMode": false
        }
      }
    },
