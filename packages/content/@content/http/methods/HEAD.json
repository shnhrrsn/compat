      "tree": {
        "type": "root",
        "children": [
          {
            "type": "element",
            "tagName": "p",
            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The "
              },
              {
                "type": "element",
                "tagName": "strong",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "HTTP "
                  },
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "HEAD"
                      }
                    ]
                  },
                  {
                    "type": "text",
                    "value": " method"
                  }
                ]
              },
              {
                "type": "text",
                "value": " requests the "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "href": "/en-US/docs/Web/HTTP/Headers"
                },
                "children": [
                  {
                    "type": "text",
                    "value": "headers"
                  }
                ]
              },
              {
                "type": "text",
                "value": " that would be returned if the "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "HEAD"
                  }
                ]
              },
              {
                "type": "text",
                "value": " request's URL was instead requested with the HTTP "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/http/methods/GET"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "GET"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " method. For example, if a URL might produce a large download, a "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "HEAD"
                  }
                ]
              },
              {
                "type": "text",
                "value": " request could read its "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/http/headers/Content-Length"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "Content-Length"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " header to check the filesize without actually downloading the file."
              }
            ]
          }
        ],
