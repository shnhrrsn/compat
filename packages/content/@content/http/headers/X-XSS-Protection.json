            "children": [
              {
                "type": "text",
                "value": "The HTTP "
              },
              {
                "type": "element",
                "tagName": "strong",
                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "X-XSS-Protection"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " response header is a feature of Internet Explorer, Chrome and Safari that stops pages from loading when they detect reflected cross-site scripting ("
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "href": "/en-US/docs/Glossary/Cross-site_scripting"
                },
                "children": [
                  {
                    "type": "text",
                    "value": "XSS"
                  }
                ]
              },
              {
                "type": "text",
                "value": ") attacks. These protections are largely unnecessary in modern browsers when sites implement a strong "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/http/headers/csp/Content-Security-Policy"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "Content-Security-Policy"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " that disables the use of inline JavaScript ("
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "'unsafe-inline'"
                  }
                ]
              },
              {
                "type": "text",
                "value": ")."
              }
            ]
          }
        ],
