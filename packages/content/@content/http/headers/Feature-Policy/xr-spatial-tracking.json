            "children": [
              {
                "type": "text",
                "value": "The HTTP "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/http/headers/Feature-Policy"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "Feature-Policy"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " header "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "xr-spatial-tracking"
                  }
                ]
              },
              {
                "type": "text",
                "value": " directive controls whether the current document is allowed to use the "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "href": "/en-US/docs/Web/API/WebXR_Device_API"
                },
                "children": [
                  {
                    "type": "text",
                    "value": "WebXR Device API"
                  }
                ]
              },
              {
                "type": "text",
                "value": ". This policy controls whether "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/XRSystem/requestSession"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "navigator.xr.requestSession()"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " can return "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/XRSession"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "XRSession"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " that requires spatial tracking and whether user agent can indicate support for sessions supporting spatial tracking via "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/XRSystem/isSessionSupported"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "navigator.xr.isSessionSupported()"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " and "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/MediaDevices/devicechange_event"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "devicechange"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " event on "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/Navigator/xr"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "navigator.xr"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " object."
              }
            ]
          }
        ],
