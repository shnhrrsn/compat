    "intro": {
      "id": "c3b82555827142175ccde732dc67e0fa",
      "tree": {
        "type": "root",
        "children": [
          {
            "type": "element",
            "tagName": "p",
            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The HTTP "
              },
              {
                "type": "element",
                "tagName": "strong",
                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "Public-Key-Pins-Report-Only"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " response header was\nused to send reports of pinning violation to the "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "report-uri"
                  }
                ]
              },
              {
                "type": "text",
                "value": " specified in\nthe header but, unlike "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/http/headers/Public-Key-Pins"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "Public-Key-Pins"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " still allows browsers to\nconnect to the server if the pinning is violated. The header is silently ignored in\nmodern browsers as support for HPKP has been removed. Use "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "href": "/en-US/docs/Web/Security/Certificate_Transparency"
                },
                "children": [
                  {
                    "type": "text",
                    "value": "Certificate Transparency"
                  }
                ]
              },
              {
                "type": "text",
                "value": "\nand the "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/http/headers/Expect-CT"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "Expect-CT"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " header instead."
              }
            ]
          }
        ],
