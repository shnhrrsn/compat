    "intro": {
      "id": "5a1ab3996475e58e7b9740fcee2a2361",
      "tree": {
        "type": "root",
        "children": [
          {
            "type": "element",
            "tagName": "p",
            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The "
              },
              {
                "type": "element",
                "tagName": "strong",
                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "Range"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " HTTP request header indicates the part of a document that the server should return. Several parts can be requested with one "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "Range"
                  }
                ]
              },
              {
                "type": "text",
                "value": " header at once, and the server may send back these ranges in a multipart document. If the server sends back ranges, it uses the "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/http/status/206"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "206 Partial Content"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " for the response. If the ranges are invalid, the server returns the "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/http/status/416"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "416 Range Not Satisfiable"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " error. The server can also ignore the "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "Range"
                  }
                ]
              },
              {
                "type": "text",
                "value": " header and return the whole document with a "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/http/status/200"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "200"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " status code."
              }
            ]
          }
        ],
