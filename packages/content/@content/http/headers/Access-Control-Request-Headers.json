    "intro": {
      "id": "384f76e1e6e902f275a676b3dcc738a0",
      "tree": {
        "type": "root",
        "children": [
          {
            "type": "element",
            "tagName": "p",
            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The "
              },
              {
                "type": "element",
                "tagName": "strong",
                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "Access-Control-Request-Headers"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " request header is used by browsers when issuing a "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "href": "/en-US/docs/Glossary/preflight_request"
                },
                "children": [
                  {
                    "type": "text",
                    "value": "preflight request"
                  }
                ]
              },
              {
                "type": "text",
                "value": " to let the server know which "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "href": "/en-US/docs/Web/HTTP/Headers"
                },
                "children": [
                  {
                    "type": "text",
                    "value": "HTTP headers"
                  }
                ]
              },
              {
                "type": "text",
                "value": " the client might send when the actual request is made (such as with "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/fetch"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "fetch()"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " or "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/api/XMLHttpRequest/setRequestHeader"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "XMLHttpRequest.setRequestHeader()"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": "). The complementary server-side header of "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/http/headers/Access-Control-Allow-Headers"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "Access-Control-Allow-Headers"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " will answer this browser-side header."
              }
            ]
          }
        ],
