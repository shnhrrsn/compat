            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The "
              },
              {
                "type": "element",
                "tagName": "strong",
                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "Last-Modified"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " response HTTP header contains a date\nand time when the origin server believes the resource was last modified. It is used\nas a validator to determine if the resource is the same as the previously stored one. Less accurate\nthan an "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/http/headers/ETag"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "ETag"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " header, it is a fallback mechanism. Conditional requests\ncontaining "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/http/headers/If-Modified-Since"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "If-Modified-Since"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " or "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/http/headers/If-Unmodified-Since"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "If-Unmodified-Since"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": "\nheaders make use of this field."
              }
            ]
          }
        ],
