            "properties": {},
            "children": [
              {
                "type": "text",
                "value": "The "
              },
              {
                "type": "element",
                "tagName": "strong",
                "properties": {},
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "Access-Control-Request-Method"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " request header is used\nby browsers when issuing a "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "href": "/en-US/docs/Glossary/preflight_request"
                },
                "children": [
                  {
                    "type": "text",
                    "value": "preflight request"
                  }
                ]
              },
              {
                "type": "text",
                "value": ", to let the server know\nwhich "
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "href": "/en-US/docs/Web/HTTP/Methods"
                },
                "children": [
                  {
                    "type": "text",
                    "value": "HTTP method"
                  }
                ]
              },
              {
                "type": "text",
                "value": " will be used when the\nactual request is made. This header is necessary as the preflight request is always an\n"
              },
              {
                "type": "element",
                "tagName": "a",
                "properties": {
                  "internal": "",
                  "href": "/http/methods/OPTIONS"
                },
                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "OPTIONS"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " and doesn't use the same method as the actual request."
              }
            ]
          }
        ],
