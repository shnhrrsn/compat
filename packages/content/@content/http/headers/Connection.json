                "children": [
                  {
                    "type": "element",
                    "tagName": "code",
                    "properties": {},
                    "children": [
                      {
                        "type": "text",
                        "value": "Connection"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "text",
                "value": " general header controls whether the\nnetwork connection stays open after the current transaction finishes. If the value sent\nis "
              },
              {
                "type": "element",
                "tagName": "code",
                "properties": {},
                "children": [
                  {
                    "type": "text",
                    "value": "keep-alive"
                  }
                ]
              },
              {
                "type": "text",
                "value": ", the connection is persistent and not closed, allowing for\nsubsequent requests to the same server to be done."
              }
            ]
          }
        ],
