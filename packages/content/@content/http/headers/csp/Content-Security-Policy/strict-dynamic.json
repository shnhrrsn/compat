                "tagName": "a",
                "properties": {
                  "href": "/en-US/docs/Web/XSLT"
                },
                "children": [
                  {
                    "type": "text",
                    "value": "XSLT stylesheets"
                  }
                ]
              },
              {
                "type": "text",
                "value": " which can trigger script execution."
              }
            ]
